
<table class="table table-striped">
  <thead>
    <tr>
      <th>Student</th>
			<th>Rank</th>
			<th>Age</th>
    </tr>
  </thead>
  <tbody>
		<% @students.each do |s| %>
			<tr>
				<td><%= link_to s.name, student_path(s) %></td>
				<td><%= rank_name(s.rank) %></td>
				<td><%= s.age %></td>
			</tr>
		<% end %>
	</tbody>
</table>